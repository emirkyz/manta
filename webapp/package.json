{
  "name": "nmf-analysis-webapp",
  "version": "1.0.0",
  "description": "Web UI for NMF Topic Modeling Analysis Results",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "install-all": "npm install && cd client && npm install"
  },
  "keywords": ["nmf", "topic-modeling", "visualization", "react", "express"],
  "author": "NMF Analysis Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "fs-extra": "^11.1.1",
    "path": "^0.12.7",
    "xlsx": "^0.18.5",
    "adm-zip": "^0.5.10",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.1"
  }
}